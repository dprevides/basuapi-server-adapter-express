{
  "name": "@basuapi/server-adapter-express",
  "version": "1.0.1",
  "main": "dist/index.js",
  "bin": "./bin/cli.js",
  "files": [
    "dist/**/*.*",
    "dist/*.*"
  ],
  "license": "MIT",
  "dependencies": {
    "@basuapi/api": "1.0.68",
    "class-transformer": "^0.5.1",
    "express": "^4.18.1",
    "reflect-metadata": "^0.1.13",
    "ts-transformer-keys": "^0.4.3"
  },
  "scripts": {
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --inspect --watch \"dist/**/*\" -e js ./dist/main.js",
    "build": "swc src -d dist",
    "start": "NODE_ENV=development node dist/main.js",
    "test": "dotenv -e .env.test -- yarn jest",
    "clean": "rm -rf dist",
    "generate": "prisma generate",
    "module:publish": "yarn clean && yarn build && yarn publish --access=public"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.218",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "concurrently": "^7.2.2",
    "dotenv-cli": "^6.0.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "ts-jest": "^28.0.7",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
